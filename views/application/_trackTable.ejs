<table class="table table-striped table-hover" data-mobile-responsive="true">
   <thead class="thead-default">
<tr>
  <th>No.</th>
  <th>Song</th>
  <th>Album</th>
  <th>Artist</th>
  <th><img src="images/duration_logo.png" alt="" width="15px"></th>
  <th>Audio</th>
  <th></th>
</tr>
</thead>
<tbody>
<% tracks.forEach((track, i, original) => { %>
  <% if (i === 0) { %>
<% }; %>
  <tr>
  <td><%= track.track_number %></td>
  <td><%= track.track_name %></td>
  <td><%= track.album_name.album_name %></td>
  <td><%= track.artist.name %></td>
  <td><%= Math.floor(track.track_duration/60)%><%= (((track.track_duration%60/100).toFixed(2)).toString()).slice(1)%></td>
  <td><audio controls><source src="<%= track.audio_path %>" type="audio/mpeg"></audio><td>
  <td><a href='/tracks/<%=track._id %>/edit'>edit</a></td>
  </tr>
<% if (i === original.length-1) { %>

<% }; %>
<% }); %>

</tbody>

</table>
