<section class="container">
<h1>artists</h1>
<% artists.forEach((artist, i, original) => { %>
  <% if (i === 0) { %>
  <div class="row">
<% }; %>
<div class="col-md-4">
<div class="thumbnail">
<div class="caption">
<h3><%= artist.name %></h3>
<p><a href='/artists/<%=artist._id %>' class='btn btn-primary' role='button'>View</a></p>
</div>
</div>
</div>
<% if (i === original.length-1) { %>
  </div>
<% }; %>
<% }); %>
</section>
